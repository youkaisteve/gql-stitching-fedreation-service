{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nest-apollo/src",
  "monorepo": true,
  "root": "apps/nest-apollo",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/nest-apollo/tsconfig.app.json"
  },
  "projects": {
    "nest-apollo": {
      "type": "application",
      "root": "apps/nest-apollo",
      "entryFile": "main",
      "sourceRoot": "apps/nest-apollo/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nest-apollo/tsconfig.app.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    },
    "application": {
      "type": "application",
      "root": "apps/application",
      "entryFile": "main",
      "sourceRoot": "apps/application/src",
      "compilerOptions": {
        "tsConfigPath": "apps/application/tsconfig.app.json"
      }
    }
  }
}